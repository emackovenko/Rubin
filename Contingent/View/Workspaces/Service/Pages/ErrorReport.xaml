      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Contingent.View.Workspaces.Service.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="522.663"
      Title="ErrorReport">
    <Grid>
        <Menu
            VerticalAlignment="Top"
            Height="21">
            <Menu.Items>
                <MenuItem
                    Header="Полный отчёт"
                    Command="{Binding Path=GenerateFullReportCommand}">
                </MenuItem>                 
            </Menu.Items>
        </Menu>
    </Grid>
</Page>
